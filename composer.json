{
  "name": "andrew72ru/yii2-flowjs",
  "description": "Flowjs uploader for Yii2",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Zhdanovskih",
      "email": "andrew72ru@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "^2.0",
    "yiisoft/yii2-bootstrap": "^2.0.0",
    "yiisoft/yii2-mongodb": "~2.1.0",
    "bower-asset/flow.js": "^2.11",
    "flowjs/flow-php-server": "^1.0"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "^2.0.0",
    "codeception/specify": "^0.4.3",
    "codeception/verify": "^0.3.1",
    "mongodb/mongodb": "^1.1.1"
  },
  "autoload": {
    "psr-4": {
      "andrew72ru\\flowjs\\": ""
    }
  },
  "extra": {
    "bootstrap": "andrew72ru\\flowjs\\Bootstrap",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "asset-vcs-driver-options": {
      "github-no-api": true
    },
    "asset-pattern-skip-version": "(-build)"
  }
}
